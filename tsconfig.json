{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@themes/*": [
        "src/themes/*"
      ],
      "@customTypes/*": [
        "src/types/*"
      ],
      "@util/*": [
        "src/util/*"
      ],
      "@helpers/*": [
        "src/helpers/*"
      ],
    }
  }
}